{
  "name": "testes-e2e-cypress",
  "version": "1.0.0",
  "description": "TBD.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint cypress/**/*.js && eslint cypress.config.js",
    "lint:fix": "eslint cypress/**/*.js --fix && eslint cypress.config.js --fix",
    "cy:open": "cypress open",
    "cy:open:tablet": "cypress open --config viewportWidth=767,viewportHeight=480",
    "test": "cypress run --reporter mocha-allure-reporter",
    "test:tablet": "cypress run --reporter mocha-allure-reporter --config viewportWidth=767,viewportHeight=480 --spec cypress/e2e/authenticated.cy.js",
    "test:cloud": "cypress run --reporter mocha-allure-reporter --record --tag 'desktop'",
    "test:tablet:cloud": "cypress run --reporter mocha-allure-reporter --record --tag 'tablet' --config viewportWidth=767,viewportHeight=480 --spec cypress/e2e/authenticated.cy.js --env grepTags=@desktop-and-tablet,grepOmitFiltered=true",
    "report:allure": "allure generate allure-results --clean -o allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/grep": "^3.1.5",
    "@faker-js/faker": "^8.0.2",
    "allure-commandline": "^2.27.0",
    "cypress": "^12.15.0",
    "cypress-iframe": "^1.0.1",
    "cypress-mailosaur": "^2.13.0",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.13.3",
    "mocha-allure-reporter": "^1.4.0"
  }
}
